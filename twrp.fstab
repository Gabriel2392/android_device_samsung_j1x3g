/boot		emmc		/dev/block/platform/sdio_emmc/by-name/KERNEL  
/cache		ext4		/dev/block/platform/sdio_emmc/by-name/CACHE  
/data		ext4		/dev/block/platform/sdio_emmc/by-name/userdata 	length=-16384
/recovery	emmc		/dev/block/platform/sdio_emmc/by-name/RECOVERY 
/system		ext4		/dev/block/platform/sdio_emmc/by-name/SYSTEM 
/efs		ext4		/dev/block/platform/sdio_emmc/by-name/efs flags=display="efs";backup=1
/wmodem		emmc		/dev/block/platform/sdio_emmc/by-name/MODEM flags=display="wmodem";backup=1
/sdcard            vfat    /dev/block/platform/sdio_sd/mmcblk1p1  /dev/block/mmcblk1   flags=display="Micro SDcard";storage;wipeingui;removable
/usb-otg         vfat       /dev/block/sda1	/dev/block/sda	flags=display="USB-OTG";storage;wipeingui;removable



